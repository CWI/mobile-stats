<input type="hidden" id="property_name" value="<%= params[:property] %>" /><div class="well">  <div class="navbar">    <div class="navbar-inner">      <form class="navbar-form pull-left">        <ul class="nav nav-pills">          <li <% if params[:property] == 'vendor' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/vendor">Vendor</a></li>          <li <% if params[:property] == 'platform' %>class="active"<%'' end %>><a href="/<%= params[:event_name] %>/report/platform">Platform</a></li>          <li <% if params[:property] == 'browser' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/browser">Browser</a></li>          <li <% if params[:property] == 'pixel_ratio' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/pixel_ratio">Pixel Ratio</a></li>          <li <% if params[:property] == 'resolution' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/resolution">Resolution</a></li>          <li <% if params[:property] == 'language' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/language">Language</a></li>          <li <% if params[:property] == 'iphone_version' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/iphone_version">iPhone Version</a></li>          <li <% if params[:property] == 'android_version' %>class="active"<% end %>><a href="/<%= params[:event_name] %>/report/android_version">Android Version</a></li>        </ul>      </form>    </div>  </div>  <div class="row-fluid">    <div id="grid-container" class="span4">      <table id="" class="table table-striped">        <thead>          <tr>            <th>Description</th>            <th>Qtd</th>            <th>Perc</th>          </tr>        </thead>        <tbody>          <tr>            <td colspan="3">Aguardando dados...</td>          </tr>        </tbody>      </table>    </div>    <div id="chart-container" class="span8">      <canvas id="chart"></canvas>    </div>  </div></div>